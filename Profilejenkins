pipeline {
    agent {
        docker {
            image 'maven:3.8.1-adoptopenjdk-11'
        }
    }
    stages {
        stage('Build and Run Java Code') {
            steps {
                sh 'mvn --version'
                sh 'javac DevOpsEngineerDetailsProfile.java'
                sh 'java DevOpsEngineerDetails'
            }
        }
    }
}
